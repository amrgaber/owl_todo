{
  "name": "owl_todo",
  "version": "0.1.0",
  "module": "main.js",
  "type": "module",
  "app_config": {
    "port": 3000,
    "public_path": "public",
    "build_path": "dist",
    "inline_css": false,
    "inline_xml": false,
    "other_static_files": {}
  },
  "scripts": {
    "prestart": "bun run build",
    "start": "bun run main.js",
    "dev": "bun run main.js --dev --scss",
    "build": "bun run core/cli.js --build",
    "scss": "sass src/styles:public/css --watch"
  },
  "devDependencies": {
    "bun-types": "latest",
    "chokidar": "^3.5.3",
    "sass": "^1.69.5"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.6.0",
    "@odoo/owl": "^2.3.0",
    "sass": "^1.69.5"
  }
}